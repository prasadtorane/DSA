public class Main {
  public static void main(String[] args) {
   int gcd =  GCD(48,18);
    System.out.println(gcd);
  }

  public static int GCD(int a,int b){
          while(b != 0) {
            int remainder = a%b;
             a = b;
             b = remainder;
          }
    return a;
  }
}
